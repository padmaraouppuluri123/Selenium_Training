import java.util.ArrayList;
import java.util.List;

public class Collection {

	public static void main(String[] args) {

		List<String> basket = new ArrayList<>();

		// Adding items
		basket.add("Apples");
		basket.add("Bananas");
		basket.add("Milk");

		// Duplicate item
		basket.add("Apples");

		System.out.println("Items in your basket: " + basket);

	}

}
