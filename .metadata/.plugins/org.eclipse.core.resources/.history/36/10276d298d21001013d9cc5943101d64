package com_variables;

public class VariablesExample {

	
	public void neha() {
		
		int age=25;
		System.out.println(age);
		
	}
	
	public void isha() {
		
		System.out.println("VariablesExample.isha()");
		
	}
	
	public void newz() {
		
		System.out.println("VariablesExample.newz()");
		
	}
	
	
	public static void main(String[] args) {
		
		// Creating an object
		
		VariablesExample cs= new VariablesExample();
		
		cs.neha();
		cs.isha();
		cs.newz();
		
		
	}
	
	
}
